{
  "name": "devsu-challenge",
  "description": "DevSu Challenge Angular",
  "author": {
    "name": "Jaime Burgos Tejada"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SkyZeroZx/devsu_challenge"
  },
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start": "nx serve",
    "start:pwa:dev": "npm run build && http-server -p 8080 -c-1 dist/devsu-challenge",
    "format": "prettier --write \"./src/**/*.{ts,json,html,scss}\"",
    "lint-format": "npm run format &&  npm run lint --fix",
    "pre-commit": "npm run lint-format && git add .",
    "build:production": "nx build --project devsu-challenge --configuration production --output-hashing=all",
    "build": "nx build",
    "test": "nx test --codeCoverage",
    "sonarqube": "sonar-scanner",
    "lint": "nx run-many -t lint",
    "prepare": "husky install"
  },
  "private": true,
  "jestSonar": {
    "reportPath": "./coverage/devsu-challenge/report",
    "reportFile": "sonar_report.xml",
    "indent": 4
  },
  "dependencies": {
    "@angular/animations": "~16.2.0",
    "@angular/common": "~16.2.0",
    "@angular/compiler": "~16.2.0",
    "@angular/core": "~16.2.0",
    "@angular/forms": "~16.2.0",
    "@angular/platform-browser": "~16.2.0",
    "@angular/platform-browser-dynamic": "~16.2.0",
    "@angular/router": "~16.2.0",
    "@angular/service-worker": "~16.2.0",
    "@nx/angular": "16.8.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~16.2.0",
    "@angular-devkit/core": "~16.2.0",
    "@angular-devkit/schematics": "~16.2.0",
    "@angular-eslint/eslint-plugin": "~16.0.0",
    "@angular-eslint/eslint-plugin-template": "~16.0.0",
    "@angular-eslint/template-parser": "~16.0.0",
    "@angular/cli": "~16.2.0",
    "@angular/compiler-cli": "~16.2.0",
    "@angular/language-service": "~16.2.0",
    "@nx/cypress": "16.8.1",
    "@nx/eslint-plugin": "16.8.1",
    "@nx/jest": "16.8.1",
    "@nx/js": "16.8.1",
    "@nx/linter": "16.8.1",
    "@nx/web": "16.8.1",
    "@nx/workspace": "16.8.1",
    "@schematics/angular": "~16.2.0",
    "@types/jest": "^29.4.0",
    "@types/node": "16.11.7",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "cypress": "^13.0.0",
    "eslint": "~8.46.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.13.4",
    "http-server": "^14.1.1",
    "husky": "^8.0.0",
    "jest": "^29.4.1",
    "jest-environment-jsdom": "^29.4.1",
    "jest-preset-angular": "~13.1.0",
    "jest-sonar-reporter": "^2.0.0",
    "jsonc-eslint-parser": "^2.1.0",
    "nx": "16.8.1",
    "prettier": "^2.6.2",
    "pretty-quick": "^3.1.3",
    "sonar-scanner": "^3.1.0",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "typescript": "~5.1.3"
  }
}